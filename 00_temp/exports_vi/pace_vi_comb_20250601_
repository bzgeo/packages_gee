var roi = ee.FeatureCollection("projects/bz-sdg/aoi/aoi_globe");

var img1 = ee.ImageCollection.fromImages([img1a,img1b,img1c,img1d]).median().divide(10000).toFloat();
var img2 = ee.ImageCollection.fromImages([img2a,img2b,img2c,img2d]).median().divide(10000).toFloat();

print(img1);
print(img2);

Export.image.toAsset({image:img1,description:'export_ee_pace_vi_1',
