/***** End of imports. If edited, may not auto-convert in the playground. *****/
// Last updated: 19.06.2025

/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

img1b = img1b.updateMask(img1b.gte(-10000)); // NDVI
img2b = img2b.updateMask(img2b.gte(-10000)); // EVI
img3b = img3b.updateMask(img3b.gte(-10000)); // CCI 
img4b = img4b.updateMask(img4b.gte(-10000)); // CIRE
img5b = img5b.updateMask(img5b.gte(-10000)); // CAR
img6b = img6b.updateMask(img6b.gte(-10000)); // mARI
img7b = img7b.updateMask(img7b.gte(-10000)); // PRI

/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

// 01: NDVI
var ndvi = ee.Image.cat([img1a,img1b.rename("b59")]);
print(ndvi);

// 02: EVI
var evi = ee.Image.cat([img2a,img2b.rename("b59")]);
print(evi);

// 05: CCI
var cci = ee.Image.cat([img3a,img3b.rename("b59")]);
print(cci);

// 07: CIRE*
var cire = ee.Image.cat([img4a,img4b.rename("b59")]);
print(cire);

// 08: CAR*
var car = ee.Image.cat([img5a,img5b.rename("b59")]);
print(car);

// 09: mARI
var mari = ee.Image.cat([img6a,img6b.rename("b59")]);
print(mari);

// 10: PRI
var pri = ee.Image.cat([img7a,img7b.rename("b59")]);
print(pri);

/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

