// 01: NDVI
var ndvi = ee.Image.cat([img1a,img1b,img1c,img1d,img1e,img1f]);
ndvi = coll(ndvi).median();

// 02: EVI
var evi = ee.Image.cat([img2a,img2b,img2c,img2d,img2e,img2f]);
evi = coll(evi).median();

// 05: CCI
var cci = ee.Image.cat([img3a,img3b,img3c,img3d,img3e,img3f]);
cci = coll(cci).median();

// 07: CIRE*
var cire = ee.Image.cat([img4a,img4b,img4c,img4d,img4e,img4f]);
cire = coll(cire).median();

// 08: CAR*
var car = ee.Image.cat([img5a,img5b,img5c,img5d,img5e,img5f]);
car = coll(car).median();

// 09: mARI
var mari = ee.Image.cat([img6a,img6b,img6c,img6d,img6e,img6f]);
mari = coll(mari).median();

// 10: PRI
var pri = ee.Image.cat([img7a,img7b,img7c,img7d,img7e,img7f]);
pri = coll(pri).median();


Map.addLayer(ndvi,{min: -10000, max: 10000},'NDVI', 1);
Map.addLayer(evi,{min: -10000, max: 10000},'EVI', 1);
Map.addLayer(cci,{min: -10000, max: 10000},'CCI', 1);
Map.addLayer(cire,{min: -10000, max: 10000},'CIRE', 1);
Map.addLayer(car,{min: -10000, max: 10000},'CAR', 1);
Map.addLayer(mari,{min: -10000, max: 10000},'mARI', 1);
Map.addLayer(pri,{min: -10000, max: 10000},'PRI', 1);