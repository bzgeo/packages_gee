    img02 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_vi/PACE_OCI_20250511_L3m_1D_LANDVI_V3"),
    img03 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_vi/PACE_OCI_20250512_L3m_1D_LANDVI_V3"),
    img04 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_vi/PACE_OCI_20250513_L3m_1D_LANDVI_V3"),
    img05 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_vi/PACE_OCI_20250514_L3m_1D_LANDVI_V3"),
    img06 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_vi/PACE_OCI_20250515_L3m_1D_LANDVI_V3"),
    img07 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_vi/PACE_OCI_20250516_L3m_1D_LANDVI_V3");
