var img1 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_vi/8day_4km/vi_01_ndvi_x48_4km"),
    img2 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_vi/8day_4km/vi_01_ndvi_x49_4km"),
    img3 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_vi/8day_4km/vi_01_ndvi_x50_4km"),
    img4 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_vi/8day_4km/vi_01_ndvi_x51_4km"),
    img5 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_vi/8day_4km/vi_01_ndvi_x52_4km");
/***** End of imports. If edited, may not auto-convert in the playground. *****/

var flt = function(img){return img.updateMask(img.gte(-10000)).updateMask(img.lte(10000))};


Map.setCenter(-88.6743, 17.3834, 9);
Map.addLayer(flt(img1),{min: -10000, max: 10000}, '1', 1);
Map.addLayer(flt(img2),{min: -10000, max: 10000}, '2', 1);
Map.addLayer(flt(img3),{min: -10000, max: 10000}, '3', 1);
Map.addLayer(flt(img4),{min: -10000, max: 10000}, '4', 1);
Map.addLayer(flt(img5),{min: -10000, max: 10000}, '5', 1);