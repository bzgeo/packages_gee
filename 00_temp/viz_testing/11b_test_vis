// Plot the fitted model and the original data at the ROI.

print(ui.Chart.image.series(c.pace_oci_bz_vis, a.bz_poly, ee.Reducer.mean(),20000)
    .setChartType('LineChart')
    //.setChartType('ScatterChart')
    .setOptions({title: 'BZ: NDVI', lineWidth: 1, pointSize: 1}));

print(ui.Chart.image.doySeries(c.pace_oci_global_car_, a.bz_poly, ee.Reducer.mean(),20000)
    .setChartType('LineChart')
    //.setChartType('ScatterChart')
    .setOptions({title: 'BZ: CAR', lineWidth: 1, pointSize: 1}));

