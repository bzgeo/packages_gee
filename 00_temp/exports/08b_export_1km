/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var img1 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_sr/mesoamerica_01km/_orig/PACE_OCI_20240309T184249_L2_SFREFL_V3_0_");
/***** End of imports. If edited, may not auto-convert in the playground. *****/

var roi = ee.Geometry.Rectangle(-77, 6, -93, 22); // Mesoamerica ROI

var img1_ = img1.select(ee.List.sequence(0,121,1)).multiply(10000).toInt16();

print(img1_);



Export.image.toAsset({image:img1_.clip(roi),description:'export_ee_pace_1km',
assetId:'projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_sr/mesoamerica_01km/mes_pace_oci_sr_v3_20240309_1km',
scale:1200,region: roi, crs: 'EPSG:4326'});
