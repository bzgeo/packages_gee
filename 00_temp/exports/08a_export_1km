/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var img1 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_sr/belize/PACE_OCI_20240309T184249_L2_SFREFL_V3_"),
    img2 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_sr/belize/PACE_OCI_20240409T184828_L2_SFREFL_V3_"),
    img3 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_sr/belize/PACE_OCI_20241119T182625_L2_SFREFL_V3_"),
    img4 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_sr/belize/PACE_OCI_20250315T183800_L2_SFREFL_V3_"),
    img5 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_sr/belize/PACE_OCI_20250316T191331_L2_SFREFL_V3_");
/***** End of imports. If edited, may not auto-convert in the playground. *****/

var roi = ee.Geometry.Rectangle(-77, 6, -93, 22); // Mesoamerica ROI

var img1_ = img1.select(ee.List.sequence(0,121,1)).multiply(10000).toInt16();
var img2_ = img2.select(ee.List.sequence(0,121,1)).multiply(10000).toInt16();
var img3_ = img3.select(ee.List.sequence(0,121,1)).multiply(10000).toInt16();
var img4_ = img4.select(ee.List.sequence(0,121,1)).multiply(10000).toInt16();
var img5_ = img5.select(ee.List.sequence(0,121,1)).multiply(10000).toInt16();

print(img1_);
print(img2_);
print(img3_);
print(img4_);
print(img5_);


Export.image.toAsset({image:img1_.clip(roi),description:'export_ee_pace_1km',
assetId:'projects/bz-sdg/compil_imagery/hyperspectral/pace_oci_sr/belize/mes_pace_oci_sr_v3_20240309_1km',
scale:1200,region: roi, crs: 'EPSG:4326'});

