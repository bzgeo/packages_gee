/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var img1 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/paci_oci/PACE_OCI_20250202_20250209_L3m_8D_LANDVI_V3"),
    img2 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/paci_oci/PACE_OCI_20250210_20250217_L3m_8D_LANDVI_V3"),
    img3 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/paci_oci/PACE_OCI_20250218_20250225_L3m_8D_LANDVI_V3"),
    img4 = ee.Image("projects/bz-sdg/compil_imagery/hyperspectral/paci_oci/PACE_OCI_20250226_20250305_L3m_8D_LANDVI_V3");
/***** End of imports. If edited, may not auto-convert in the playground. *****/

var img_coll = ee.ImageCollection.fromImages([img1, img2, img3, img4]);
print(img_coll);

var img5 = img_coll.median();
print(img5);

Export.image.toAsset({image:img5,description:'export_ee_pace',
assetId:'projects/bz-sdg/compil_imagery/hyperspectral/paci_oci/PACE_OCI_202502x_L3m_MO_LANDVI_V3',scale:4638.312214700219,
region:img5.geometry().bounds(), crs: 'EPSG:4326'});